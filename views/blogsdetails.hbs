<main id="content">
<div class="container px-md-5">
<nav aria-label="breadcrumb">
<ol class="breadcrumb pt-5 pb-3 flex-nowrap flex-md-wrap overflow-auto overflow-md-hidden">

</ol>
</nav>




<div class="row mb-5 mb-lg-6">
<div class="col-lg-8">
<div class="mb-6">
<div class="mb-6">
<a href="#" class="d-block mb-4">
<img class="img-fluid" src="{{bd.thumbnail}}" alt="Image-Description">
</a>
<h6 class="font-size-26 font-weight-semi-bold mb-1">
{{bd.title}}</h6>
<div class="mb-3 pb-1">
<a href="#" class="font-weight-semi-bold font-size-13">
<span>KYT</span>
</a>
<a href="#" class="product-comment font-weight-normal font-size-13 font-italic h-g-primary">
<span>{{bd.update_date}}</span>
</a>
</div>
{{{bd.description}}}
</div>





</div>
</div>
<div class="col-lg-4">
<div class="pl-lg-6">
        





<div class="mb-5 dark">
<header class="d-md-flex align-items-center justify-content-between mb-3 pb-1 w-100 home-section">
<h6 class="font-size-20 font-weight-medium m-0 text-gray-700">Latest Article</h6>
<div class="border-top col p-0 mx-3 border-gray-3600"></div>
<a href="/blogs" class="h-g-primary font-size-1 font-weight-medium font-secondary">View All</a>
</header>
<div class="row">

{{#each latest_blogs}}
<div class="col-md-6 col-lg-12">
<div class="position-relative mb-3">
<div class="bg-img-hero bg-gradient__1" style="background-image: url({{thumbnail}}); height: 180px;">
<div class="position-absolute bottom-0 z-index-2 pl-3 ml-1 pr-7 mb-3">
<h6 class="product-title font-size-18 text-lh-1dot3 font-weight-semi-bold font-secondary">
<a href="/bdetails/{{url}}">{{title}}</a>
</h6>
<div class="text-lh-sm">
<a href="/bdetails/{{url}}" class="font-weight-bold font-size-13 font-italic">
</a>
<a href="/bdetails/{{url}}" class="product-comment font-weight-normal font-size-13 font-italic h-g-primary mr-9">
<span>{{update_date}}</span>
</a>
</div>
</div>
</div>
</div>
</div>
{{/each}}






</div>
</div>








</div>
</div>
</div>
</div>
</main>